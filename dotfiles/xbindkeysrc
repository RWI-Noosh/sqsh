"amixer set Master toggle"
    XF86AudioMute

# Raise and Lower are swapped on my Air's keyboard, it seems.
"amixer set Master 3db-"
    XF86AudioRaiseVolume

"amixer set Master 3db+"
    XF86AudioLowerVolume

"sudo bash -c 'echo $(($(cat /sys/class/backlight/intel_backlight/brightness)-200)) > /sys/class/backlight/intel_backlight/brightness'"
    XF86MonBrightnessDown

"sudo bash -c 'echo $(($(cat /sys/class/backlight/intel_backlight/brightness)+200)) > /sys/class/backlight/intel_backlight/brightness'"
    XF86MonBrightnessUp
